{
  "name": "tree-sitter-yuho",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for the Yuho legal statute DSL",
  "main": "bindings/node",
  "keywords": [
    "tree-sitter",
    "parser",
    "yuho",
    "legal",
    "dsl"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gongahkia/yuho.git"
  },
  "author": "gongahkia",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.22.0"
  },
  "scripts": {
    "build": "tree-sitter generate && tree-sitter build",
    "test": "tree-sitter test",
    "parse": "tree-sitter parse"
  },
  "tree-sitter": [
    {
      "scope": "source.yuho",
      "file-types": [
        "yh"
      ],
      "injection-regex": "yuho"
    }
  ]
